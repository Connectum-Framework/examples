{
  "name": "@connectum/example-basic-service",
  "version": "0.2.0-alpha.2",
  "private": true,
  "description": "Basic example service using @connectum/core",
  "type": "module",
  "imports": {
    "#gen/*": "./gen/*",
    "#*": "./src/*"
  },
  "scripts": {
    "start": "node src/index.ts",
    "dev": "node --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "buf:generate": "buf generate",
    "buf:lint": "buf lint",
    "build:proto": "pnpm run buf:generate",
    "test": "node --test tests/**/*.test.ts"
  },
  "keywords": ["connectrpc", "grpc", "connectum", "example"],
  "author": "Highload.Zone",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.11.0",
    "@connectrpc/connect": "^2.1.1",
    "@connectum/core": "1.0.0-rc.3",
    "@connectum/healthcheck": "1.0.0-rc.3",
    "@connectum/interceptors": "1.0.0-rc.3",
    "@connectum/reflection": "1.0.0-rc.3"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.65.0",
    "@bufbuild/protoc-gen-es": "^2.11.0",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bufbuild/buf"
    ]
  }
}
