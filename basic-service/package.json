{
  "name": "@connectum/example-basic-service",
  "version": "0.2.0-alpha.2",
  "private": true,
  "description": "Basic example service using @connectum/core",
  "type": "module",
  "imports": {
    "#gen/*": "./gen/*",
    "#*": "./src/*"
  },
  "scripts": {
    "start": "node src/index.ts",
    "dev": "node --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "build:proto": "pnpm exec protoc -I proto --es_out=gen --es_opt=target=js+dts --connect-es_out=gen --connect-es_opt=target=js+dts proto/*.proto",
    "test": "node --test tests/**/*.test.ts"
  },
  "keywords": ["connectrpc", "grpc", "connectum", "example"],
  "author": "Highload.Zone",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=25.2.0"
  },
  "dependencies": {
    "@connectum/core": "workspace:^",
    "@connectum/healthcheck": "workspace:^",
    "@connectum/reflection": "workspace:^",
    "@connectrpc/connect": "catalog:",
    "@bufbuild/protobuf": "catalog:"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "catalog:",
    "typescript": "catalog:"
  }
}
