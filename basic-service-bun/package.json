{
  "name": "@connectum/example-basic-service-bun",
  "version": "0.1.0",
  "private": true,
  "description": "Basic Connectum service example â€” Bun runtime",
  "type": "module",
  "imports": {
    "#gen/*": "./gen/*",
    "#*": "./src/*"
  },
  "scripts": {
    "start": "bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "buf:generate": "buf generate",
    "buf:lint": "buf lint",
    "build:proto": "pnpm run buf:generate",
    "test": "node --test tests/e2e/**/*.test.ts",
    "test:bun": "bun test tests/e2e/"
  },
  "keywords": ["connectrpc", "grpc", "connectum", "example", "bun"],
  "author": "Highload.Zone",
  "license": "Apache-2.0",
  "dependencies": {
    "@bufbuild/protobuf": "^2.11.0",
    "@connectrpc/connect": "^2.1.1",
    "@connectum/core": "1.0.0-rc.4",
    "@connectum/healthcheck": "1.0.0-rc.4",
    "@connectum/interceptors": "1.0.0-rc.4",
    "@connectum/reflection": "1.0.0-rc.4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.65.0",
    "@bufbuild/protoc-gen-es": "^2.11.0",
    "@connectrpc/connect-node": "^2.1.1",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bufbuild/buf"
    ]
  }
}
