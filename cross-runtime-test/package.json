{
  "name": "cross-runtime-test",
  "version": "0.1.0",
  "private": true,
  "description": "Cross-runtime smoke tests for compiled @connectum/* packages",
  "type": "module",
  "imports": {
    "#gen/*": "./gen/*",
    "#*": "./src/*"
  },
  "scripts": {
    "build:proto": "buf generate",
    "test": "node --test tests/**/*.test.ts",
    "test:node": "node --test tests/**/*.test.ts",
    "test:bun": "bun test tests/",
    "test:all": "npm run test:node && npm run test:bun",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["connectrpc", "grpc", "connectum", "cross-runtime", "smoke-test"],
  "author": "Highload.Zone",
  "license": "Apache-2.0",
  "dependencies": {
    "@bufbuild/protobuf": "^2.11.0",
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-node": "^2.1.1",
    "@connectum/core": "1.0.0-rc.4",
    "@connectum/healthcheck": "1.0.0-rc.4",
    "@connectum/interceptors": "1.0.0-rc.4",
    "@connectum/reflection": "1.0.0-rc.4"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.65.0",
    "@bufbuild/protoc-gen-es": "^2.11.0",
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@bufbuild/buf"]
  }
}
